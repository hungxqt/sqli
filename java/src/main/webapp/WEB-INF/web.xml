<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <display-name>SQL Injection Labs</display-name>
    
    <welcome-file-list>
        <welcome-file>home</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>sqli.servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnionServlet</servlet-name>
        <servlet-class>sqli.servlets.UnionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReflectServlet</servlet-name>
        <servlet-class>sqli.servlets.ReflectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BooleanServlet</servlet-name>
        <servlet-class>sqli.servlets.BooleanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeServlet</servlet-name>
        <servlet-class>sqli.servlets.TimeServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnionServlet</servlet-name>
        <url-pattern>/union</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReflectServlet</servlet-name>
        <url-pattern>/reflect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BooleanServlet</servlet-name>
        <url-pattern>/boolean</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeServlet</servlet-name>
        <url-pattern>/time</url-pattern>
    </servlet-mapping>
    
</web-app>
